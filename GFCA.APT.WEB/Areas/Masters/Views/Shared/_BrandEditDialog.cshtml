@model GFCA.APT.Domain.Dto.BrandDto
@using Syncfusion.EJ2

<div>
    <div class="form-row">
        <div class="form-group col-md-6">
            <div class="e-float-input e-control-wrapper">
                @Html.TextBox("BRAND_ID", Model.BRAND_ID.ToString(), new { disabled = true })
                <span class="e-float-line"></span>
                @Html.Label("BRAND_ID", "Brand ID", new { @class = "e-float-text e-label-top" })
            </div>
        </div>
    </div>

    <div class="form-row">
        <div class="form-group col-md-6">
            <div class="e-float-input e-control-wrapper">
                @Html.TextBox("BRAND_CODE", Model.BRAND_CODE?? string.Empty, new { disabled = true })
                <span class="e-float-line"></span>
                @Html.Label("BRAND_CODE", "Brand Code", new { @class = "e-float-text e-label-top" })
            </div>
        </div>
        <div class="form-group col-md-6">
            <div class="e-float-input e-control-wrapper">
                @Html.TextBox("BRAND_NAME", Model.BRAND_NAME?? string.Empty)
                <span class="e-float-line"></span>
                @Html.Label("BRAND_NAME", "Brand Name", new { @class = "e-float-text e-label-top" })
            </div>
        </div>
    </div>

</div>

@Html.EJS().ScriptManager()
