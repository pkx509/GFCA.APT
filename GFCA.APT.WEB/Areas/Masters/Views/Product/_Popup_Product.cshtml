                                    <label class="col-sm-3 col-md-2 col-lg-3 control-label">
                                        Customer
                                    </label>
                                    <div class="col-sm-9 col-md-10 col-lg-9">

                                        @(Html.EJS().ComboBox("pop-cmb-cust_code")
                                                .Placeholder("-- Select --")
                                                .Autofill(true)
                                                .DataSource(o =>
                                                {
                                                    o.Url("http://localhost:57628/api/Selection/GetCustomer")
                                                    .CrossDomain(false)
                                                    .Adaptor("UrlAdaptor");
                                                })


                                                .Fields(new Syncfusion.EJ2.DropDowns.ComboBoxFieldSettings { Text = "Text", Value = "Value" })
                                                .Render())
                                    </div>

                                </div>
                            </div>
                            <div class="col-sm-12 col-lg-6">
                                <div class="form-group row">

                                    <label class="col-sm-3 col-md-2 col-lg-3 control-label">
