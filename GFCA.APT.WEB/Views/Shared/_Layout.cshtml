@using Syncfusion.EJ2
<!DOCTYPE html>
<html lang="en-us">
<head>
    <meta charset="utf-8" />
    <meta http-equiv="content-type" content="text/html; charset=utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - APT</title>

    @Styles.Render("~/Content/cssLte")

    @Styles.Render("~/Content/ej2/bootstrap4.css")
    @Scripts.Render("~/bundles/modernizr")
    @Scripts.Render("~/bundles/polyfill")
    @Scripts.Render("~/Scripts/ej2/ej2.min.js")
    <style>
        /*icon styles */
        @@font-face {
            font-family: 'fontello';
            src: url('data:application/octet-stream;base64,') format('truetype');
        }
        /*icon styles */
    </style>
    @RenderSection("styles", required: false)
</head>
<body class="hold-transition sidebar-mini sidebar-mini-xs sidebar-mini-md sidebar-collapse layout-fixed layout-navbar-fixed layout-footer-fixed text-sm">
    <div class="wrapper">

        <!-- Preloader -->
        <div class="preloader flex-column justify-content-center align-items-center">
            <img class="animation__shake" src="~/Content/dist/img/AsiaticAPTLogo.png" alt="Asiatic APT Logo" height="60" width="60" />
        </div>

        <!-- Navbar -->
        @Html.Partial("_Navbar")
        <!-- /.navbar -->
        <!-- Main Sidebar Container -->
        @Html.Partial("_Sidebar")

        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper">
            @RenderBody()
        </div>
        <!-- /.content-wrapper -->
        @Html.Partial("_Footer")

        <!-- Control Sidebar -->
        <aside class="control-sidebar control-sidebar-light">
            <!-- Control sidebar content goes here -->
        </aside>
        <!-- /.control-sidebar -->
    </div>
    
    @Scripts.Render("~/bundles/jquerylte")
    <!-- Resolve conflict in jQuery UI tooltip with Bootstrap tooltip -->
    <script>
        $.widget.bridge('uibutton', $.ui.button)
    </script>
    @Scripts.Render("~/bundles/adminlte")
    @RenderSection("scripts", required: false)
    @Html.EJS().ScriptManager()
</body>
</html>